<?xml version="1.0"?>

<PropertyList>
	<path>gear.ac</path>
	<offsets>
	  <x-m>0</x-m>
	  <y-m>0</y-m>
	  <z-m>0</z-m>
	</offsets>
	
  <animation>
		<type>select</type>
		<object-name>front.lowersusp</object-name>
		<object-name>front.uppersusp</object-name>
		<object-name>left.lowersusp</object-name>
		<object-name>left.uppersusp</object-name>
		<object-name>right.lowersusp</object-name>
		<object-name>right.uppersusp</object-name>
		<object-name>front.gear</object-name>
		<object-name>right.gear</object-name>
		<object-name>left.gear</object-name>
		<condition>
			<greater-than>
				<property>gear/gear[0]/position-norm</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	
	<!-- Retracting the gears -->
	<!-- Front gear -->
	<animation>
		<type>rotate</type>
		<object-name>front.uppersusp</object-name>
		<object-name>front.lowersusp</object-name>
		<object-name>front.gear</object-name>
		<property>gear/gear[0]/position-norm</property>
		<factor>90</factor>
		<offset-deg>-90</offset-deg>
		<center>
			<x-m>3.9324</x-m>
			<y-m>0</y-m>
			<z-m>1.3514</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>front.uppersusp</object-name>
		<object-name>front.lowersusp</object-name>
		<object-name>front.gear</object-name>
		<property>gear/gear[0]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>-90</dep>
			</entry>
			<entry>
				<ind>0.2</ind>
				<dep>-90</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<offset-deg>-90</offset-deg>
		<center>
			<x-m>3.9324</x-m>
			<y-m>0</y-m>
			<z-m>1.3514</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<!-- Left gear -->
    <animation>
		<type>rotate</type>
		<object-name>left.uppersusp</object-name>
		<object-name>left.lowersusp</object-name>
		<object-name>left.gear</object-name>
		<property>gear/gear[1]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>-90</dep>
			</entry>
			<entry>
				<ind>0.1</ind>
				<dep>-90</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<offset-deg>-90</offset-deg>
		<center>
			<x-m>8.8258</x-m>
			<y-m>-1.045</y-m>
			<z-m>1.7096</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>left.uppersusp</object-name>
		<object-name>left.lowersusp</object-name>
		<object-name>left.gear</object-name>
		<property>gear/gear[1]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>-90</dep>
			</entry>
			<entry>
				<ind>0.4</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<offset-deg>-90</offset-deg>
		<center>
			<x-m>8.8258</x-m>
			<y-m>-1.045</y-m>
			<z-m>1.7096</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>-1</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>left.uppersusp</object-name>
		<object-name>left.lowersusp</object-name>
		<object-name>left.gear</object-name>
		<property>gear/gear[1]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>-5</dep>
			</entry>
			<entry>
				<ind>0.4</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<offset-deg>-90</offset-deg>
		<center>
			<x-m>8.8258</x-m>
			<y-m>-1.045</y-m>
			<z-m>1.7096</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>left.uppersusp</object-name>
		<object-name>left.lowersusp</object-name>
		<object-name>left.gear</object-name>
		<property>gear/gear[1]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0.8</dep>
			</entry>
			<entry>
				<ind>0.2</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<!-- Right gear -->
	<animation>
		<type>rotate</type>
		<object-name>right.uppersusp</object-name>
		<object-name>right.lowersusp</object-name>
		<object-name>right.gear</object-name>
		<property>gear/gear[2]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>-90</dep>
			</entry>
			<entry>
				<ind>0.1</ind>
				<dep>-90</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<offset-deg>-90</offset-deg>
		<center>
			<x-m>8.8258</x-m>
			<y-m>1.045</y-m>
			<z-m>1.7096</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>right.uppersusp</object-name>
		<object-name>right.lowersusp</object-name>
		<object-name>right.gear</object-name>
		<property>gear/gear[2]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>90</dep>
			</entry>
			<entry>
				<ind>0.4</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<offset-deg>-90</offset-deg>
		<center>
			<x-m>8.8258</x-m>
			<y-m>1.045</y-m>
			<z-m>1.7096</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>-1</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>right.uppersusp</object-name>
		<object-name>right.lowersusp</object-name>
		<object-name>right.gear</object-name>
		<property>gear/gear[2]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>5</dep>
			</entry>
			<entry>
				<ind>0.4</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<offset-deg>-90</offset-deg>
		<center>
			<x-m>8.8258</x-m>
			<y-m>1.045</y-m>
			<z-m>1.7096</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>right.uppersusp</object-name>
		<object-name>right.lowersusp</object-name>
		<object-name>right.gear</object-name>
		<property>gear/gear[2]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0.8</dep>
			</entry>
			<entry>
				<ind>0.2</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<!-- compression -->
	<animation>
		<condition>
			<equals>
			<property>gear/gear[0]/position-norm</property>
			<value>1</value>
			</equals>
		</condition>
		<type>translate</type>
		<object-name>front.lowersusp</object-name>
		<object-name>front.gear</object-name>
		<property>gear/gear[0]/compression-norm</property>
		<factor>0.32</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<condition>
			<equals>
			<property>gear/gear[1]/position-norm</property>
			<value>1</value>
			</equals>
		</condition>
		<type>translate</type>
		<object-name>left.lowersusp</object-name>
		<object-name>left.gear</object-name>
		<property>gear/gear[1]/compression-norm</property>
		<factor>0.3</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<condition>
			<equals>
			<property>gear/gear[2]/position-norm</property>
			<value>1</value>
			</equals>
		</condition>
		<type>translate</type>
		<object-name>right.lowersusp</object-name>
		<object-name>right.gear</object-name>
		<property>gear/gear[2]/compression-norm</property>
		<factor>0.3</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>

  <!-- Steering  -->	
	<animation>
        <type>rotate</type>
        <object-name>front.gear</object-name>
        <object-name>front.lowersusp</object-name>
        <property>controls/flight/rudder</property>
        <factor>-50</factor>
		<center>
      <x-m>3.932</x-m>
      <y-m>0</y-m>
      <z-m>0.823</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>
	
	<animation>
  <type>spin</type>
  <object-name>front.gear</object-name>
  <property>gear/gear[0]/rollspeed-ms</property>
  <factor> 30 </factor>
  <center>
   <x-m>3.89826</x-m>
   <y-m>0</y-m>
   <z-m>0.212263</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>spin</type>
  <object-name>left.gear</object-name>
  <property>gear/gear[1]/rollspeed-ms</property>
  <factor> 30 </factor>
  <center>
   <x-m>8.826483</x-m>
   <y-m>-1.3159</y-m>
   <z-m>0.245195</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>spin</type>
  <object-name>right.gear</object-name>
  <property>gear/gear[2]/rollspeed-ms</property>
  <factor> 30 </factor>
  <center>
   <x-m>8.826483</x-m>
   <y-m>1.3159</y-m>
   <z-m>0.245195</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>
	
</PropertyList>